        <precision>10</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Shipment_Quantity__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Enter the Quantity to be Shipped</inlineHelpText>
        <label>Planned Shipments</label>
        <precision>7</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Under_Minimum_Qty__c</fullName>
        <description>Checks tick when quantity at inventory is less than product level minimum quantity</description>
        <externalId>false</externalId>
        <formula>( (OnHand_Qty__c + Receivable_Qty__c) - Shipment_Quantity__c )&lt;= Product__r.Min_Qty__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Under Minimum Qty</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <listViews>
        <fullName>All</fullName>
        <filterScope>Everything</filterScope>
