<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Under_Minimum_Qty__c</fullName>
    <description>Checks tick when quantity at inventory is less than product level minimum quantity</description>
    <externalId>false</externalId>
    <formula>( (OnHand_Qty__c + Receivable_Qty__c) - Shipment_Quantity__c )&lt;= Product__r.Min_Qty__c</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Under Minimum Qty</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
